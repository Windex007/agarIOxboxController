<Window x:Name="loaded" x:Class="Agar360.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agar360"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="loaded_Loaded">
    <Grid>
        <Button x:Name="button" Content="Start" Margin="50,0,0,10" VerticalAlignment="Bottom" Click="button_Click" Height="38" HorizontalAlignment="Left" Width="150"/>
        <Button x:Name="button1" Content="Stop" Margin="0,0,50,10" HorizontalAlignment="Right" Width="150" Height="38" VerticalAlignment="Bottom" Click="button1_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="146" Margin="35,24,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="442"/>

    </Grid>
</Window>
